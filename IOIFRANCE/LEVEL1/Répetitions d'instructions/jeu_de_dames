#include <iostream>
#define repeat(nb) for (int _loop = 1, _max = (nb) ; _loop <= _max ; _loop++)
using namespace std;

int main()
{
   for (int i=0;i<20;i++) {

   for (int j=0;j<10;j++) {
      cout<<"OXOX";
   }
   cout<<endl;
   for (int k=0;k<10;k++) {
      cout<<"XOXO";
   }
   cout<<endl;

   }
