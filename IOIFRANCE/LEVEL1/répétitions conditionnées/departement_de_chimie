#include <iostream>
#define repeat(nb) for (int _loop = 1, _max = (nb) ; _loop <= _max ; _loop++)
#include <string>
using namespace std;

int main()
{
  int n ;
  int minn , maxx ; 
  cin>>n>>minn>>maxx ;
  


  for(int i=0;i<n;i++){
    int x; 
    cin>>x ; 
    if (x>=minn && x<=maxx ) 
        cout<<"Rien Ã  signaler"<<endl;
    else 
        {cout<<"Alerte !!" <<endl; break ; }
   
  }
}
