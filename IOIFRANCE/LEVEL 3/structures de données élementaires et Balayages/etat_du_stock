#include <bits/stdc++.h>

using namespace std;
int main()
{
 int n ;
 cin>>n ;
 map<int , int > m ;
 for (int i=1;i<=n;i++){
    int x ;
    cin>>x;
    m[i]=x;
 }
 int t;
 cin>>t;
 for (int i=0;i<t;i++){
    int a,b;
    cin>>a>>b ;

    m[a]+=b;
 }
 for (auto x: m ){
    cout<<x.second<<" ";
 }
}
