#include<bits/stdc++.h>

using namespace std ;

int main()
{
 int n;
 cin>>n  ;
 for (int i=0;i<n;i++){
    double x;
    char ch ;
    cin>>x>>ch;
    if (ch=='m')
        cout<<x/0.3048<<" p"<<endl ;
    else
        if(ch=='g')
            cout<<x*0.002205 <<" l"<<endl;
    else
            cout<<x*1.8+32<< " f"<<endl;
 }
}
