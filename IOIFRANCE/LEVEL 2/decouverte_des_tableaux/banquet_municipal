#include<bits/stdc++.h>

using namespace std ;

int main()
{
    unsigned int n,p ;
    cin>>n>>p ;
    int t[n];
    for (int i=0;i<n;i++){
        int x;
        cin>>x;
        t[i]=x;
    }
    for (int i=0;i<p;i++){
        int x,y ;
        cin>>x>>y;
        swap(t[x],t[y]);
    }
    for (int i=0;i<n;i++){
     cout<<t[i]<<endl;
    }

}
