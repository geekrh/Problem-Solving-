#include <bits/stdc++.h>

using namespace std;
int main()
{
   int n ;
   cin>>n ;
   int t[n];
   for (int i=0;i<n;i++)
   {
       int x ;
       cin>>x;
       if (x==1)
        t[i]=2;
       else if (x==2)
        t[i]=1;
       else if (x==3)
        t[i]=3;
        else if (x==4)
            t[i]=5;
        else if (x==5)
            t[i]=4;
   }
   for (int i=n-1;i>=0;i--){
        cout<<t[i]<<endl;
   }

}
